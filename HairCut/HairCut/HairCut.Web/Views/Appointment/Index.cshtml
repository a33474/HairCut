@using HairCut.ViewModels;
@{
    ViewData["Title"] = "Index";
}
<table class="table table-bordered table-striped">
    <tr>
        <th>Client</th>
        <th>Appointment DateOfCreation</th>
        <th>Appointment StartTime</th>
        <th>Employee</th>
    </tr>
        @for (int i = 0; i < Model.Rows.Count; i++)
        {
    <tr>
        <td>@Model.Rows[i][1]</td>
        <td>@Model.Rows[i][2]</td>
        <td>@Model.Rows[i][3]</td>
        <td>@Model.Rows[i][4]</td>
    </tr>
        }
</table>
<a href="@Url.Action("Create","Appointment")">Add Appointment</a>

<h2>Index</h2>

